<?xml version="1.0" encoding="utf-8"?>
<layout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

<androidx.constraintlayout.widget.ConstraintLayout

    android:id="@+id/frameLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".QualityFragment">

    <TextView
        android:id="@+id/tv_questions"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="36dp"
        android:text="@string/Questions"
        android:textSize="18sp"
        android:textColor="@android:color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.176"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/iv_first_index"
        android:layout_width="@dimen/_64dp"
        android:layout_height="@dimen/_64dp"
        android:contentDescription="@string/image_quality"
        android:src="@drawable/ic_sleep_1"
        app:layout_constraintBottom_toBottomOf="@+id/iv_zero_index"
        app:layout_constraintEnd_toStartOf="@+id/iv_second_index"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/iv_zero_index"
        app:layout_constraintTop_toTopOf="@+id/iv_zero_index" />

    <ImageView
        android:id="@+id/iv_zero_index"
        android:layout_width="@dimen/_64dp"
        android:layout_height="@dimen/_64dp"
        android:layout_marginTop="28dp"
        android:contentDescription="@string/image_quality"
        android:src="@drawable/ic_sleep_0"
        app:layout_constraintEnd_toStartOf="@+id/iv_first_index"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_questions" />

    <ImageView
        android:id="@+id/iv_second_index"
        android:layout_width="@dimen/_64dp"
        android:layout_height="@dimen/_64dp"
        android:contentDescription="@string/image_quality"
        android:src="@drawable/ic_sleep_2"
        app:layout_constraintBottom_toBottomOf="@+id/iv_first_index"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/iv_first_index"
        app:layout_constraintTop_toTopOf="@+id/iv_first_index" />

    <ImageView
        android:id="@+id/iv_thirt"
        android:layout_width="@dimen/_64dp"
        android:layout_height="@dimen/_64dp"
        android:layout_marginTop="32dp"
        android:contentDescription="@string/image_quality"
        android:src="@drawable/ic_sleep_3"
        app:layout_constraintEnd_toStartOf="@+id/imageView2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iv_zero_index" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="@dimen/_64dp"
        android:layout_height="@dimen/_64dp"
        android:contentDescription="@string/image_quality"
        android:src="@drawable/ic_sleep_4"
        app:layout_constraintBottom_toBottomOf="@+id/iv_thirt"
        app:layout_constraintEnd_toStartOf="@+id/imageView3"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/iv_thirt"
        app:layout_constraintTop_toTopOf="@+id/iv_thirt" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="@dimen/_64dp"
        android:layout_height="@dimen/_64dp"
        android:contentDescription="@string/image_quality"
        android:src="@drawable/ic_sleep_5"
        app:layout_constraintBottom_toBottomOf="@+id/iv_thirt"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/imageView2"
        app:layout_constraintTop_toTopOf="@+id/iv_thirt" />
</androidx.constraintlayout.widget.ConstraintLayout>

</layout>